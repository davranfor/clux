{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/users",
  "type": "object",
  "properties": {
    "GET": {
      "if": {
        "properties": {
          "path": { "items": [{}, {"x-mask": "0999999"}, { "const": "edit" }], "additionalItems": false },
          "content": { "type": "null" }
        },
        "x-notify": { "path": "users.select.edit", "size": 2, "role": 1 }
      },
      "then": true, 
      "else": false
    }
  }
}

