{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/users.schema",
  "definitions": {
    "update.token": {
      "type": "object",
      "properties": {
        "email": { "type": "string", "format": "email" },
        "password": { "type": "string", "minLength": 8, "maxLength": 25 }
      },
      "additionalProperties": false,
      "required": [ "email", "password" ]
    }
  }
}

