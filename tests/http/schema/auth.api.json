{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/auth",
  "type": "object",
  "if": {
    "properties": {
      "GET": {
        "properties": {
          "path": { "minItems": 1, "maxItems": 1 },
          "query": { "type": "null" },
          "content": { "type": "null" },
          "user": { "type": "integer", "minimum": 1 }
        }
      }
    },
    "additionalProperties": false,
    "x-notify": { "path": "auth.get", "role": 1 }
  },
  "then": true,
  "else": false
}

